
@{
    ViewBag.Title = "Index";
}

<div class="panel panel-primary">
    <div class="panel-heading">用户信息</div>
   <table class="table table-striped">
       @foreach (var item in Model.Keys)
       {
        <tr>
            <th>@item</th>
            <td>@Model[item]</td>
        </tr>
       }
    </table>
</div>
@Html.ActionLink("注销", "Logout", "Account", null, new { @class = "btn btn-primary" })